{
  "manifest_version": 2,
  "name": "Bot Tasker",
  "description": "Google Chrome extension for deploying Automation Anywhere Bots",
  "version": "1.0",
  "browser_action": {
      "default_icon": "assets/images/icon.png",
      "default_popup": "index.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "storage"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": [
      "assets/css/*",
      "assets/js/*",
      "assets/fonts/*"
  ]
}
